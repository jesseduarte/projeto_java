package oo.heranca.desafio;

public class Carro {

	int velocidadeAtual;
	
	Carro(int velocidadeAtual) {
		this.velocidadeAtual = velocidadeAtual;
	}
	
	public void acelerar() {
		if(this.velocidadeAtual >= 0) {
			this.velocidadeAtual += 5;
		}
	}
	
	public void frear() {
		if(this.velocidadeAtual >= 0) {
			this.velocidadeAtual -= 5;
		}
	}
}
