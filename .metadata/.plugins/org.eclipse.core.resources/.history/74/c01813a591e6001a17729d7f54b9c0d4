package classe;

public class ProdutoStaticMain {

	public static void main(String[] args) {
		
		ProdutoStatic p1 = new ProdutoStatic("Notebook", 2000.00);
		System.out.println(p1.precoComDesconto());
		
		ProdutoStatic.desconto = 0.20;
		
		ProdutoStatic p2 = new ProdutoStatic("Caneta", 2.00);
		System.out.println(p2.precoComDesconto());
		
	}
}
