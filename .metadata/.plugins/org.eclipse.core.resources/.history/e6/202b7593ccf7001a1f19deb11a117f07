package streams;

public class Teste {
	
	public static void main(String[] args) {
		
		String s = "Jessé";
		char[] stringConvertida = s.toCharArray();
		stringConvertida[0] = 'D';
		for(char c: stringConvertida) {
			System.out.println(c);
		}
		s = stringConvertida.toString();
		System.out.println(s);
		
		for (int i = 0; i < s.length(); i++) {
			//System.out.println(s);
		}
	}

}
