		String s = "Jessé";
		char[] stringConvertida = s.toCharArray();
		stringConvertida[0] = 'D';
		for (int i = 0; i < stringConvertida.length; i++) {
			s += stringConvertida[i];
			System.out.println(s);
		}
		
		
		for (int i = 0; i < s.length(); i++) {
			//System.out.println(s);
		}
	}

}
