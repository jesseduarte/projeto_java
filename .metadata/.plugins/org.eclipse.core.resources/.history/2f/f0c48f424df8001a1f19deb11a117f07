package streams;

public class MediaTeste {

	public static void main(String[] args) {
		
		Media m1 = new Media().adicionar(8.3).adicionar(6.7); //Posso fazer isso por causa do return this no met adicionar()
		Media m2 = new Media().adicionar(7.9).adicionar(6.6);

		System.out.println(m1.getValor());
		System.out.println(m2.getValor());
		
		Media resultante = Media.combinar(m1, m2);
		
		System.out.println(resultante);
	}
}
