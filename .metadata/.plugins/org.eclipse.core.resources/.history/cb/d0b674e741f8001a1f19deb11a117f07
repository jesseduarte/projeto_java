package streams;

//Classe utilizada junto com a classe DesafioFilter

public class Produto {

	final String nome;
	final double preco;
	final double desconto;
	final Double frete;
	
	public Produto(String nome, double preco, double desconto, Double frete) {
		this.nome = nome;
		this.preco = preco;
		this.desconto = desconto;
		this.frete = frete;
	}	
}