package streams;

//Será usada junto com a classe Reduce3

public class Media {

	private double total;
	private int quantidade;
	
	public Media adicionar(double valor) {
		total += valor;
		quantidade++;
		return this; //ele sempre retornará o objeto, toda vez que chamo essa funcao.
	}
	
	public double getValor() {
		return total/quantidade;
	}
	
	public Media combinar() {
		
	}
}