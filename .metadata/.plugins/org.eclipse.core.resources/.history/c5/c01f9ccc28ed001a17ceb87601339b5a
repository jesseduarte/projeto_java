package metodos;

import java.util.ArrayList;
import java.util.List;

public class Usuario {

	String nome;
	int idade;
	List<Usuario> lista = new ArrayList<>();
	
	Usuario(String nome, int idade) {
		this.nome = nome;
		this.idade = idade;
	} 
	
	public int atribuirIdade(int idade) {
		this.idade = idade;
		return idade;
	}
}
