package exception.checkedException.test;

import java.io.File;
import java.io.IOException;

public class CheckedExceptionTest {
	
	public static void main(String[] args) {
		
		criarArquivo();
	}
	
	public static void criarArquivo() {
		//IOException é uma classe que é checked (que deve ser tratada, nao compila senao tratar!)
		File file = new File("teste.txt");
		
		try{
			file.createNewFile();
			System.out.println("Arquivo criado");
		} catch(IOException e) { // e é um objeto do tipo IOException
			e.printStackTrace(); //retorna uma pilha de métodos da exception
		}
	}

}
